// { "env": { "production": { "plugins": ["@emotion"] } }, "plugins": ["@emotion"] }

{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ],
    "@babel/preset-env"
  ],
  "plugins": [
    // "inline-react-svg",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@src": "./src"
        },
        "extensions": [".js", ".jsx", ".tsx", ".ts"]
      }
    ],
    "@emotion/babel-plugin"
  ],
  "env": { "production": { "plugins": ["@emotion"] } }
}
